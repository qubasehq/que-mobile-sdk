{
    "name": "que-expo",
    "version": "0.1.0",
    "description": "Expo/React Native bridge for QUE Mobile SDK",
    "main": "lib/commonjs/index",
    "module": "lib/module/index",
    "types": "lib/typescript/index.d.ts",
    "react-native": "src/index",
    "source": "src/index",
    "files": [
        "src",
        "lib",
        "android",
        "cpp",
        "app.plugin.js",
        "expo-module.config.json"
    ],
    "scripts": {
        "build": "bob build",
        "clean": "del-cli lib",
        "lint": "eslint \"**/*.{js,ts,tsx}\" --prettier",
        "release": "release-it",
        "test": "jest",
        "typescript": "tsc --noEmit"
    },
    "keywords": [
        "react-native",
        "expo",
        "que",
        "agent",
        "ai"
    ],
    "repository": "https://github.com/your-repo/que-mobile-sdk",
    "author": "QUE Team",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/your-repo/que-mobile-sdk/issues"
    },
    "homepage": "https://github.com/your-repo/que-mobile-sdk#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^17.0.2",
        "@evilmartians/lefthook": "^1.5.0",
        "@react-native/eslint-config": "^0.72.2",
        "@release-it/conventional-changelog": "^5.0.0",
        "@types/jest": "^28.1.2",
        "@types/react": "~17.0.21",
        "@types/react-native": "0.70.0",
        "commitlint": "^17.0.2",
        "del-cli": "^5.0.0",
        "eslint": "^8.4.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^28.1.1",
        "prettier": "^2.0.5",
        "react": "18.2.0",
        "react-native": "0.72.4",
        "react-native-builder-bob": "^0.20.0",
        "release-it": "^15.0.0",
        "typescript": "^5.0.2"
    },
    "dependencies": {
        "@expo/config-plugins": "^7.9.2"
    },
    "peerDependencies": {
        "react": "*",
        "react-native": "*"
    },
    "engines": {
        "node": ">= 16.0.0"
    },
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            "commonjs",
            "module",
            [
                "typescript",
                {
                    "project": "tsconfig.build.json"
                }
            ]
        ]
    },
    "overrides": {
        "@react-native-community/cli@<=17.0.1": "17.0.1",
        "@react-native-community/cli-server-api@<=17.0.1": "17.0.1"
    }
}
